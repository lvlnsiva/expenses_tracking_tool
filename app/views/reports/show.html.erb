<table class="table table-bordered">
      <tr>
       <th>spent on</th>
       <th>category id</th>
       <th>cost</th>
       <th>item name</th>
      </tr>
        
      <% @expenses.each do |expense| %>
           <tr>
              <td><%= expense.spent_on %></td>
              <td><%= expense.catergory_id %></td>
              <td><%= expense.cost %></td>
              <td><%= expense.item_name %></td>
            </tr>
      <% end %>
  </table>